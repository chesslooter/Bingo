{
  "Id": "NumberOfPlayers",
  "Source": [
    "HeroesBingoData.json"
  ],
  "Header": "Select how many players are in your party.",
  "Buttons": [
    {
      "ButtonName": "Singleplayer: 1 player",
      "ButtonValue": "IsSingleplayer",
      "OnClickEvent": "togglePartySizeOptions('IsSingleplayer')"
    },
    {
      "ButtonName": "Party: 2-4 players",
      "ButtonValue": "IsParty",
      "OnClickEvent": "togglePartySizeOptions('IsParty')"
    },
    {
      "ButtonName": "5-Stack: 5 players",
      "ButtonValue": "IsMultiplayer",
      "OnClickEvent": "togglePartySizeOptions('IsMultiplayer')"
    }
  ]
}
